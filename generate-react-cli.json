{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "style": "src/templates/default/TemplateName.scss",
        "component": "src/templates/default/TemplateName.tsx",
        "index": "src/templates/default/index.ts",
        "story": "src/templates/default/TemplateName.stories.tsx",
        "test": "src/templates/default/TemplateName.test.tsx"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false,
      "withIndex": true
    },
    "hook": {
      "customTemplates": {
        "index": "src/templates/hook/index.ts",
        "component": "src/templates/hook/useTemplateName.ts",
        "test": "src/templates/hook/useTemplateName.test.ts"
      },
      "path": "src/shared/hooks",
      "withIndex": true,
      "withTest": true
    }
  }
}
